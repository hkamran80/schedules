        "extends": [
            "plugin:vue/essential",
            "eslint:recommended",
            "@vue/prettier"
        ],
        "parserOptions": {
            "parser": "babel-eslint"
        },
        "rules": {}
    },
    "prettier": {
        "tabWidth": 4,
        "singleQuote": false
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "last 2 Chrome versions",
        "not dead"
